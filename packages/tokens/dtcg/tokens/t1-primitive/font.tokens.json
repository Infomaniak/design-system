{
  "font": {
    "family": {
      "$type": "fontFamily",
      "sans": {
        "$value": "Inter"
      },
      "serif": {
        "$value": "Georgia"
      },
      "mono": {
        "$value": "Menlo"
      }
    },
    "size": {
      "$type": "dimension",
      "xs": {
        "$value": {
          "value": 12,
          "unit": "px"
        }
      },
      "sm": {
        "$value": {
          "value": 14,
          "unit": "px"
        }
      },
      "base": {
        "$value": {
          "value": 16,
          "unit": "px"
        }
      },
      "lg": {
        "$value": {
          "value": 18,
          "unit": "px"
        }
      },
      "xl": {
        "$value": {
          "value": 20,
          "unit": "px"
        }
      }
    },
    "weight": {
      "$type": "fontWeight",
      "thin": {
        "$value": 100
      },
      "extralight": {
        "$value": 200
      },
      "light": {
        "$value": 300
      },
      "normal": {
        "$value": 400
      },
      "medium": {
        "$value": 500
      },
      "semibold": {
        "$value": 600
      },
      "bold": {
        "$value": 700
      },
      "extrabold": {
        "$value": 800
      },
      "black": {
        "$value": 900
      }
    }
  }
}
