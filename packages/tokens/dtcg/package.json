{
  "name": "@infomaniak-design-system/tokens-dtcg",
  "version": "0.0.0",
  "type": "module",
  "main": "./index.js",
  "scripts": {
    "import": "node scripts/import-from-figma.ts",
    "normalize-and-build": "yarn run normalize && yarn run build",
    "normalize": "node scripts/normalize.ts",
    "build": "yarn build:light && yarn build:dark && yarn build:light-mode && yarn build:dark-mode && yarn build:mail-mode",
    "build:light": "tz build -c configs/terrazzo.light.config.js",
    "build:dark": "tz build -c configs/terrazzo.dark.config.js",
    "build:light-mode": "tz build -c configs/terrazzo.light-mode.config.js",
    "build:dark-mode": "tz build -c configs/terrazzo.dark-mode.config.js",
    "build:mail-mode": "tz build -c configs/terrazzo.mail-mode.config.js"
  },
  "description": "Infomaniak's Design System - Tokens",
  "keywords": [],
  "private": true,
  "devDependencies": {
    "@terrazzo/cli": "^2.0.0-alpha.7",
    "@terrazzo/plugin-css": "^2.0.0-alpha.7",
    "@terrazzo/plugin-tailwind": "^0.3.3"
  }
}
