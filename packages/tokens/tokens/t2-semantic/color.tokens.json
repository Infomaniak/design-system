{
  "color": {
    "brand": {
      "$root": {
        "$value": "{color.sky.600}",
        "$extensions": {
          "$mode": {
            "light": "{color.sky.600}",
            "dark": "{color.sky.800}",
            "drive": "{color.brand.drive}",
            "mail": "{color.brand.mail}"
          }
        }
      },
      "mail": {
        "$value": "{color.pink.400}",
        "$extensions": {
          "$mode": {
            "light": "{color.pink.400}",
            "dark": "{color.pink.600}"
          }
        }
      },
      "drive": {
        "$value": "{color.purple.700}",
        "$extensions": {
          "$mode": {
            "light": "{color.purple.700}",
            "dark": "{color.purple.950}"
          }
        }
      }
    }
  }
}
