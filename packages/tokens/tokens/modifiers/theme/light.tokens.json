{
  "$type": "color",
  "color": {
    "elevation": {
      "surface": {
        "pressed": {
          "$value": "{color.gray.100}"
        },
        "hover": {
          "$value": "{color.gray.50}"
        },
        "default": {
          "$value": "{color.white}"
        }
      },
      "overlay": {
        "default": {
          "$value": "{color.gray.100}"
        },
        "hover": {
          "$value": "{color.gray.200}"
        },
        "pressed": {
          "$value": "{color.gray.300}"
        }
      },
      "raised": {
        "default": {
          "$value": "{color.white}"
        },
        "hover": {
          "$value": "{color.gray.50}"
        },
        "pressed": {
          "$value": "{color.gray.100}"
        }
      }
    },
    "background": {
      "error": {
        "default": {
          "$value": "{color.red.600}"
        },
        "hover": {
          "$value": "{color.red.700}"
        },
        "pressed": {
          "$value": "{color.red.800}"
        }
      },
      "warning": {
        "default": {
          "$value": "{color.orange.600}"
        },
        "hover": {
          "$value": "{color.orange.700}"
        },
        "pressed": {
          "$value": "{color.orange.800}"
        }
      },
      "brand": {
        "default": {
          "$value": "{color.sky.700}"
        },
        "pressed": {
          "$value": "{color.sky.900}"
        },
        "hover": {
          "$value": "{color.sky.800}"
        }
      },
      "success": {
        "default": {
          "$value": "{color.lime.600}"
        },
        "hover": {
          "$value": "{color.lime.700}"
        },
        "pressed": {
          "$value": "{color.lime.800}"
        }
      }
    },
    "text": {
      "brand": {
        "default": {
          "$value": "{color.sky.700}"
        },
        "hover": {
          "$value": "{color.sky.800}"
        },
        "pressed": {
          "$value": "{color.sky.900}"
        }
      },
      "primary": {
        "$value": "{color.gray.800}"
      },
      "secondary": {
        "$value": "{color.gray.500}"
      },
      "tertiary": {
        "$value": "{color.gray.400}"
      },
      "inverse": {
        "$value": "{color.gray.100}"
      },
      "disabled": {
        "$value": "{color.gray.300}"
      }
    }
  }
}
